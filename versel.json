{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "build/**",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}